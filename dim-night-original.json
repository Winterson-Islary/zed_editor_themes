                "created": "#34d058ff",
                "created.background": "#101010ff",
                "created.border": "#34d058ff",

                "deleted": "#f87171ff",
                "deleted.background": "#101010ff",
                "deleted.border": "#f87171ff",

                "error": "#f87171ff",
                "error.background": "#101010ff",
                "error.border": "#f87171ff",

                "warning": "#f8ca73ff",
                "warning.background": "#101010ff",
                "warning.border": "#f8ca73ff",

                "info": "#55d9c9ff",
                "info.background": "#101010ff",
                "info.border": "#55d9c9ff",

                "hint": "#55d9c9ff",
                "hint.background": "#101010ff",
                "hint.border": "#55d9c9ff",

                "players": [
                    {
                        "cursor": "#ffffffff",
                        "background": "#ffffffff",
                        "selection": "#ffffff33"
                    }
                ],

                "syntax": {
                    "comment": {
                        "color": "#60e67dff",
                        "font_style": "italic"
                    },
                    "keyword": { "color": "#bbb0ffff" },
                    "storage": { "color": "#bbb0ffff" },
                    "type": { "color": "#71e4f6ff" },
                    "number": { "color": "#f8ca73ff" },
                    "constant": { "color": "#f8ca73ff" },
                    "string": { "color": "#c8e997ff" },
                    "variable": { "color": "#ffffffff" },
                    "property": { "color": "#5ac6faff" },
                    "function": { "color": "#ffa86bff" },
                    "operator": { "color": "#d6d3d1ff" },
                    "tag": { "color": "#fe6767ff" },
                    "attribute": { "color": "#f8ca73ff" },
                    "boolean": { "color": "#f8ca73ff" },
                    "punctuation": { "color": "#d6d3d1ff" },
                    "punctuation.bracket": {
                        "color": "#d6d3d1ff"
                    },
                    "punctuation.delimiter": {
                        "color": "#d6d3d1ff"
                    },
                    "string.regex": { "color": "#89ddffff" },
                    "string.escape": { "color": "#89ddffff" },
                    "text.literal": { "color": "#c8e997ff" },
                    "link_text": {
                        "color": "#8bb1ffff",
                        "font_style": "italic"
                    },
                    "link_uri": { "color": "#71e4f6ff" },
                    "emphasis": {
                        "color": "#fe6767ff",
                        "font_style": "italic"
                    },
                    "emphasis.strong": {
                        "color": "#fe6767ff",
                        "font_weight": 700
                    },
                    "title": {
                        "color": "#e1e4e8ff",
                        "font_weight": 700
                    },
                    "variable.readonly": { "color": "#dbdbdbff" },
                    "variable.parameter": {
                        "color": "#55d9c9ff"
                    },
                    "decorator": {
                        "color": "#8bb1ffff",
                        "font_style": "italic"
                    },
                    "namespace": { "color": "#dbdbdbff" }
