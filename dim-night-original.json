                "editor.document_highlight.write_background": "#3392ff44",

                "terminal.background": "#101010ff",
                "terminal.foreground": "#d1d5daff",
                "terminal.bright_foreground": "#e1e4e8ff",
                "terminal.dim_foreground": "#727b86ff",
                "terminal.ansi.black": "#0b0b0bff",
                "terminal.ansi.red": "#f87171ff",
                "terminal.ansi.green": "#34d058ff",
                "terminal.ansi.yellow": "#f8ca73ff",
                "terminal.ansi.blue": "#7c3aedff",
                "terminal.ansi.magenta": "#f472b6ff",
                "terminal.ansi.cyan": "#55d9c9ff",
                "terminal.ansi.white": "#e1e4e8ff",
