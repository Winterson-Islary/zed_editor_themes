{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Dim Night Elevate",
    "author": "Winterson",
    "themes": [
        {
            "name": "Dim Night Elevate",
            "appearance": "dark",
            "style": {
                "border": "#0d0d0dff",
                "border.variant": "#101010ff",
                "border.focused": "#7c3aedff",
                "border.selected": "#7c3aedff",
                "border.transparent": "#00000000",
                "border.disabled": "#1a1a1aff",

                "elevated_surface.background": "#0b0b0bff",
                "surface.background": "#0b0b0bff",
                "background": "#101010ff",

                "element.background": "#0b0b0bff",
                "element.hover": "#0e0e0eff",
                "element.active": "#1a1a1aff",
                "element.selected": "#1a1a1aff",
                "element.disabled": "#0b0b0bff",

                "text": "#e1e4e8ff",
                "text.muted": "#959da5ff",
                "text.placeholder": "#727b86ff",
                "text.disabled": "#727b86ff",
                "text.accent": "#7c3aedff",

                "icon": "#e1e4e8ff",
                "icon.muted": "#959da5ff",
                "icon.disabled": "#727b86ff",
                "icon.placeholder": "#727b86ff",
                "icon.accent": "#7c3aedff",

                "status_bar.background": "#0b0b0bff",
                "title_bar.background": "#0b0b0bff",
                "title_bar.inactive_background": "#0b0b0bff",
                "toolbar.background": "#101010ff",

                "tab_bar.background": "#0b0b0bff",
                "tab.inactive_background": "#0b0b0bff",
                "tab.active_background": "#0e0e0eff",

                "search.match_background": "#3392ff44",
                "panel.background": "#0b0b0bff",
                "panel.focused_border": "#7c3aedff",

                "scrollbar.thumb.background": "#727b8633",
                "scrollbar.thumb.hover_background": "#727b8644",
                "scrollbar.thumb.border": "#0d0d0dff",
                "scrollbar.track.background": "#00000000",
                "scrollbar.track.border": "#0d0d0dff",

                "editor.foreground": "#e1e4e8ff",
                "editor.background": "#101010ff",
                "editor.gutter.background": "#101010ff",
                "editor.subheader.background": "#0b0b0bff",
