{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Dim Night Elevate – Nordic Frost",
    "author": "Winterson",
    "themes": [
        {
            "name": "Dim Night Elevate – Nordic Frost",
            "appearance": "dark",
            "style": {
                "border": "#0d0f11ff",
                "border.variant": "#16191cff",
                "border.focused": "#7ecbffff",
                "border.selected": "#7ecbffff",
                "border.transparent": "#00000000",
                "border.disabled": "#1a1d20ff",

                "elevated_surface.background": "#0b0d0fff",
                "surface.background": "#0b0d0fff",
                "background": "#0f1113ff",

                "element.background": "#0b0d0fff",
                "element.hover": "#131619ff",
                "element.active": "#1b1f22ff",
                "element.selected": "#1b1f22ff",
                "element.disabled": "#0b0d0fff",

                "text": "#e8ecf2ff",
                "text.muted": "#9aa4afff",
                "text.placeholder": "#77808aff",
                "text.disabled": "#6e7680ff",
                "text.accent": "#7ecbffff",

                "icon": "#e8ecf2ff",
                "icon.muted": "#9aa4afff",
                "icon.disabled": "#6e7680ff",
                "icon.placeholder": "#6e7680ff",
                "icon.accent": "#7ecbffff",

                "status_bar.background": "#0b0d0fff",
                "title_bar.background": "#0b0d0fff",
                "title_bar.inactive_background": "#0b0d0fff",
                "toolbar.background": "#101215ff",

                "tab_bar.background": "#0b0d0fff",
                "tab.inactive_background": "#0b0d0fff",
                "tab.active_background": "#15171aff",

                "search.match_background": "#7ecbff44",
                "panel.background": "#0b0d0fff",
                "panel.focused_border": "#7ecbffff",

                "scrollbar.thumb.background": "#9aa4af33",
                "scrollbar.thumb.hover_background": "#9aa4af44",
                "scrollbar.thumb.border": "#0d0f11ff",
                "scrollbar.track.background": "#00000000",
                "scrollbar.track.border": "#0d0f11ff",

                "editor.foreground": "#e8ecf2ff",
                "editor.background": "#0f1113ff",
                "editor.gutter.background": "#0f1113ff",
                "editor.subheader.background": "#0b0d0fff",
                "editor.active_line.background": "#191c1fff",
                "editor.highlighted_line.background": "#191c1fff",
                "editor.line_number": "#4c5560ff",
                "editor.active_line_number": "#e8ecf2ff",
                "editor.hover_line_number": "#d6dce2ff",
                "editor.invisible": "#48515bff",
                "editor.wrap_guide": "#323a4233",
                "editor.active_wrap_guide": "#48515b44",
                "editor.document_highlight.read_background": "#7ecbff22",
                "editor.document_highlight.write_background": "#7ecbff44",

                "terminal.background": "#0f1113ff",
                "terminal.foreground": "#dee2e6ff",
                "terminal.bright_foreground": "#e8ecf2ff",
                "terminal.dim_foreground": "#77808aff",

                "terminal.ansi.black": "#0b0d0fff",
                "terminal.ansi.red": "#ff7a7aff",
                "terminal.ansi.green": "#6df2a6ff",
                "terminal.ansi.yellow": "#ffe38eff",
                "terminal.ansi.blue": "#7ecbffff",
                "terminal.ansi.magenta": "#caaaffff",
                "terminal.ansi.cyan": "#9ff6ffff",
                "terminal.ansi.white": "#e8ecf2ff",

                "terminal.ansi.bright_black": "#4c5560ff",
                "terminal.ansi.bright_red": "#ff8b9aff",
                "terminal.ansi.bright_green": "#7fffc3ff",
                "terminal.ansi.bright_yellow": "#ffefb9ff",
                "terminal.ansi.bright_blue": "#99d8ffff",
                "terminal.ansi.bright_magenta": "#dccaffff",
                "terminal.ansi.bright_cyan": "#c8feffff",
                "terminal.ansi.bright_white": "#ffffffff",

                "link_text.hover": "#7ecbffff",

                "conflict": "#ffe38eff",
                "conflict.background": "#191c1fff",
                "conflict.border": "#7ecbffff",

                "created": "#6df2a6ff",
                "created.background": "#0f1113ff",
                "created.border": "#6df2a6ff",

                "deleted": "#ff7a7aff",
                "deleted.background": "#0f1113ff",
                "deleted.border": "#ff7a7aff",

                "error": "#ff7a7aff",
                "error.background": "#0f1113ff",
                "error.border": "#ff7a7aff",

                "warning": "#ffe38eff",
                "warning.background": "#0f1113ff",
                "warning.border": "#ffe38eff",

                "info": "#9ff6ffff",
                "info.background": "#0f1113ff",
                "info.border": "#9ff6ffff",

                "hint": "#9ff6ffff",
                "hint.background": "#0f1113ff",
                "hint.border": "#9ff6ffff",

                "players": [
                    {
                        "cursor": "#ffffffff",
                        "background": "#ffffffff",
                        "selection": "#ffffff33"
                    }
                ],

                "syntax": {
                    "comment": { "color": "#9aa4afff", "font_style": "italic" },
                    "keyword": { "color": "#a8c7ffff" },
                    "storage": { "color": "#a8c7ffff" },
                    "type": { "color": "#9ff6ffff" },
                    "number": { "color": "#ffe8aaff" },
                    "constant": { "color": "#ffe8aaff" },
                    "string": { "color": "#d4ffdeff" },
                    "variable": { "color": "#ffffffff" },
                    "property": { "color": "#88d9ffff" },
                    "function": { "color": "#b0e1ffff" },
                    "operator": { "color": "#e0e3e7ff" },
                    "tag": { "color": "#ff9ba0ff" },
                    "attribute": { "color": "#ffe8aaff" },
                    "boolean": { "color": "#ffe8aaff" },
                    "punctuation": { "color": "#e0e3e7ff" },
                    "punctuation.bracket": { "color": "#e0e3e7ff" },
                    "punctuation.delimiter": { "color": "#e0e3e7ff" },
                    "string.regex": { "color": "#c3f8ffff" },
                    "string.escape": { "color": "#c3f8ffff" },
                    "text.literal": { "color": "#d4ffdeff" },
                    "link_text": {
                        "color": "#bcd7ffff",
                        "font_style": "italic"
                    },
                    "link_uri": { "color": "#9ff6ffff" },
                    "emphasis": {
                        "color": "#ff9ba0ff",
                        "font_style": "italic"
                    },
                    "emphasis.strong": {
                        "color": "#ff9ba0ff",
                        "font_weight": 700
                    },
                    "title": { "color": "#e8ecf2ff", "font_weight": 700 },
                    "variable.readonly": { "color": "#e6e9edff" },
                    "variable.parameter": { "color": "#9ff6ffff" },
                    "decorator": {
                        "color": "#bcd7ffff",
                        "font_style": "italic"
                    },
                    "namespace": { "color": "#e6e9edff" }
                }
            }
        }
    ]
}
