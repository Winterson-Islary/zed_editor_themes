{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Dim Night Elevate – Neo Dracula OLED",
    "author": "Winterson",
    "themes": [
        {
            "name": "Dim Night Elevate – Neo Dracula OLED",
            "appearance": "dark",
            "style": {
                "border": "#000000ff",
                "border.variant": "#080808ff",
                "border.focused": "#a67cffff",
                "border.selected": "#a67cffff",
                "border.transparent": "#00000000",
                "border.disabled": "#0d0d0dff",

                "elevated_surface.background": "#000000ff",
                "surface.background": "#000000ff",
                "background": "#000000ff",

                "element.background": "#000000ff",
                "element.hover": "#111111ff",
                "element.active": "#1a1a1aff",
                "element.selected": "#1a1a1aff",
                "element.disabled": "#000000ff",

                "text": "#f8f8f2ff",
                "text.muted": "#8b8fa8ff",
                "text.placeholder": "#6d7186ff",
                "text.disabled": "#6d7186ff",
                "text.accent": "#a67cffff",

                "icon": "#f8f8f2ff",
                "icon.muted": "#8b8fa8ff",
                "icon.disabled": "#6d7186ff",
                "icon.placeholder": "#6d7186ff",
                "icon.accent": "#a67cffff",

                "status_bar.background": "#000000ff",
                "title_bar.background": "#000000ff",
                "title_bar.inactive_background": "#000000ff",
                "toolbar.background": "#000000ff",

                "tab_bar.background": "#000000ff",
                "tab.inactive_background": "#000000ff",
                "tab.active_background": "#111111ff",

                "search.match_background": "#50fa7b33",
                "panel.background": "#000000ff",
                "panel.focused_border": "#a67cffff",

                "scrollbar.thumb.background": "#7d819033",
                "scrollbar.thumb.hover_background": "#7d819044",
                "scrollbar.thumb.border": "#000000ff",
                "scrollbar.track.background": "#00000000",
                "scrollbar.track.border": "#000000ff",

                "editor.foreground": "#f8f8f2ff",
                "editor.background": "#000000ff",
                "editor.gutter.background": "#000000ff",
                "editor.subheader.background": "#000000ff",
                "editor.active_line.background": "#141414ff",
                "editor.highlighted_line.background": "#141414ff",
                "editor.line_number": "#4b4b4bff",
                "editor.active_line_number": "#f8f8f2ff",
                "editor.hover_line_number": "#dfdfdfff",
                "editor.invisible": "#5a5a5aff",
                "editor.wrap_guide": "#33333333",
                "editor.active_wrap_guide": "#5a5a5a44",
                "editor.document_highlight.read_background": "#8be9fd33",
                "editor.document_highlight.write_background": "#ff79c644",

                "terminal.background": "#000000ff",
                "terminal.foreground": "#f8f8f2ff",
                "terminal.bright_foreground": "#ffffff",
                "terminal.dim_foreground": "#7d8190ff",

                "terminal.ansi.black": "#000000ff",
                "terminal.ansi.red": "#ff5555ff",
                "terminal.ansi.green": "#50fa7bff",
                "terminal.ansi.yellow": "#f1fa8cff",
                "terminal.ansi.blue": "#6272a4ff",
                "terminal.ansi.magenta": "#ff79c6ff",
                "terminal.ansi.cyan": "#8be9fdff",
                "terminal.ansi.white": "#f8f8f2ff",

                "terminal.ansi.bright_black": "#4b4b4bff",
                "terminal.ansi.bright_red": "#ff6e6eff",
                "terminal.ansi.bright_green": "#69ff95ff",
                "terminal.ansi.bright_yellow": "#f6ffafff",
                "terminal.ansi.bright_blue": "#7a8dc7ff",
                "terminal.ansi.bright_magenta": "#ff92dfff",
                "terminal.ansi.bright_cyan": "#aff3ffff",
                "terminal.ansi.bright_white": "#ffffffff",

                "link_text.hover": "#a67cffff",

                "conflict": "#f1fa8cff",
                "conflict.background": "#141414ff",
                "conflict.border": "#a67cffff",

                "created": "#50fa7bff",
                "created.background": "#000000ff",
                "created.border": "#50fa7bff",

                "deleted": "#ff5555ff",
                "deleted.background": "#000000ff",
                "deleted.border": "#ff5555ff",

                "error": "#ff5555ff",
                "error.background": "#000000ff",
                "error.border": "#ff5555ff",

                "warning": "#f1fa8cff",
                "warning.background": "#000000ff",
                "warning.border": "#f1fa8cff",

                "info": "#8be9fdff",
                "info.background": "#000000ff",
                "info.border": "#8be9fdff",

                "hint": "#8be9fdff",
                "hint.background": "#000000ff",
                "hint.border": "#8be9fdff",

                "players": [
                    {
                        "cursor": "#ffffffff",
                        "background": "#ffffffff",
                        "selection": "#ffffff33"
                    }
                ],

                "syntax": {
                    "comment": { "color": "#6272a4ff", "font_style": "italic" },
                    "keyword": { "color": "#bd93f9ff" },
                    "storage": { "color": "#bd93f9ff" },
                    "type": { "color": "#8be9fdff" },
                    "number": { "color": "#f1fa8cff" },
                    "constant": { "color": "#f1fa8cff" },
                    "string": { "color": "#f1fa8cff" },
                    "variable": { "color": "#f8f8f2ff" },
                    "property": { "color": "#8be9fdff" },
                    "function": { "color": "#50fa7bff" },
                    "operator": { "color": "#ff79c6ff" },
                    "tag": { "color": "#ff5555ff" },
                    "attribute": { "color": "#f1fa8cff" },
                    "boolean": { "color": "#f1fa8cff" },
                    "punctuation": { "color": "#e0e0e0ff" },
                    "punctuation.bracket": { "color": "#e0e0e0ff" },
                    "punctuation.delimiter": { "color": "#e0e0e0ff" },
                    "string.regex": { "color": "#8be9fdff" },
                    "string.escape": { "color": "#8be9fdff" },
                    "text.literal": { "color": "#f1fa8cff" },
                    "link_text": {
                        "color": "#bd93f9ff",
                        "font_style": "italic"
                    },
                    "link_uri": { "color": "#8be9fdff" },
                    "emphasis": {
                        "color": "#ff79c6ff",
                        "font_style": "italic"
                    },
                    "emphasis.strong": {
                        "color": "#ff79c6ff",
                        "font_weight": 700
                    },
                    "title": { "color": "#f8f8f2ff", "font_weight": 700 },
                    "variable.readonly": { "color": "#dadadaff" },
                    "variable.parameter": { "color": "#8be9fdff" },
                    "decorator": {
                        "color": "#bd93f9ff",
                        "font_style": "italic"
                    },
                    "namespace": { "color": "#dadadaff" }
                }
            }
        }
    ]
}
