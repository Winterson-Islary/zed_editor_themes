{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Dim Night Elevate – Nordic Fusion",
    "author": "Winterson",
    "themes": [
        {
            "name": "Dim Night Elevate – Nordic Fusion",
            "appearance": "dark",
            "style": {
                "border": "#0d0f11ff",
                "border.variant": "#15191cff",
                "border.focused": "#7ecbffff",
                "border.selected": "#7ecbffff",
                "border.transparent": "#00000000",
                "border.disabled": "#1a1d20ff",

                "elevated_surface.background": "#0b0b0bff",
                "surface.background": "#0b0b0bff",
                "background": "#101010ff",

                "element.background": "#0b0b0bff",
                "element.hover": "#111315ff",
                "element.active": "#1a1d20ff",
                "element.selected": "#1a1d20ff",
                "element.disabled": "#0b0b0bff",

                "text": "#e8ecf2ff",
                "text.muted": "#a5aeb7ff",
                "text.placeholder": "#7e8791ff",
                "text.disabled": "#6c757eff",
                "text.accent": "#7ecbffff",

                "icon": "#e8ecf2ff",
                "icon.muted": "#a5aeb7ff",
                "icon.disabled": "#6c757eff",
                "icon.placeholder": "#6c757eff",
                "icon.accent": "#7ecbffff",

                "status_bar.background": "#0b0b0bff",
                "title_bar.background": "#0b0b0bff",
                "title_bar.inactive_background": "#0b0b0bff",
                "toolbar.background": "#101010ff",

                "tab_bar.background": "#0b0b0bff",
                "tab.inactive_background": "#0b0b0bff",
                "tab.active_background": "#15181aff",

                "search.match_background": "#7ecbff44",
                "panel.background": "#0b0b0bff",
                "panel.focused_border": "#7ecbffff",

                "scrollbar.thumb.background": "#8fa4b533",
                "scrollbar.thumb.hover_background": "#8fa4b544",
                "scrollbar.thumb.border": "#0d0f11ff",
                "scrollbar.track.background": "#00000000",
                "scrollbar.track.border": "#0d0f11ff",

                "editor.foreground": "#e8ecf2ff",
                "editor.background": "#101010ff",
                "editor.gutter.background": "#101010ff",
                "editor.subheader.background": "#0b0b0bff",
                "editor.active_line.background": "#191c1fff",
                "editor.highlighted_line.background": "#191c1fff",
                "editor.line_number": "#4d5760ff",
                "editor.active_line_number": "#e8ecf2ff",
                "editor.hover_line_number": "#d4d9e0ff",
                "editor.invisible": "#46505aff",
                "editor.wrap_guide": "#2f363d33",
                "editor.active_wrap_guide": "#46505a44",
                "editor.document_highlight.read_background": "#7ecbff33",
                "editor.document_highlight.write_background": "#7ecbff55",

                "terminal.background": "#101010ff",
                "terminal.foreground": "#d7dce0ff",
                "terminal.bright_foreground": "#e8ecf2ff",
                "terminal.dim_foreground": "#7e8791ff",

                "terminal.ansi.black": "#0b0b0bff",
                "terminal.ansi.red": "#ff7a7aff",
                "terminal.ansi.green": "#6df2a6ff",
                "terminal.ansi.yellow": "#ffe299ff",
                "terminal.ansi.blue": "#7ecbffff",
                "terminal.ansi.magenta": "#c7aaffff",
                "terminal.ansi.cyan": "#9ff6ffff",
                "terminal.ansi.white": "#e8ecf2ff",

                "terminal.ansi.bright_black": "#4d5760ff",
                "terminal.ansi.bright_red": "#ff8b9aff",
                "terminal.ansi.bright_green": "#7fffc3ff",
                "terminal.ansi.bright_yellow": "#fff0baff",
                "terminal.ansi.bright_blue": "#9ad8ffff",
                "terminal.ansi.bright_magenta": "#e0caffff",
                "terminal.ansi.bright_cyan": "#c8feffff",
                "terminal.ansi.bright_white": "#ffffffff",

                "link_text.hover": "#7ecbffff",

                "players": [
                    {
                        "cursor": "#ffffffff",
                        "background": "#ffffffff",
                        "selection": "#ffffff22"
                    }
                ],

                "syntax": {
                    "comment": { "color": "#8ea1b3ff", "font_style": "italic" },

                    "keyword": { "color": "#a8c7ffff" },
                    "storage": { "color": "#a8c7ffff" },

                    "type": { "color": "#9ff6ffff" },

                    "number": { "color": "#ffe299ff" },
                    "constant": { "color": "#ffe299ff" },

                    "string": { "color": "#d4ffdeff" },

                    "variable": { "color": "#ffffffff" },

                    "property": { "color": "#88d9ffff" },

                    "function": { "color": "#b0e1ffff" },

                    "operator": { "color": "#dfe4e8ff" },

                    "tag": { "color": "#ff9aa0ff" },

                    "attribute": { "color": "#ffe299ff" },

                    "boolean": { "color": "#ffe299ff" },

                    "punctuation": { "color": "#dfe4e8ff" },
                    "punctuation.bracket": { "color": "#dfe4e8ff" },
                    "punctuation.delimiter": { "color": "#dfe4e8ff" },

                    "string.regex": { "color": "#c3f8ffff" },
                    "string.escape": { "color": "#c3f8ffff" },

                    "text.literal": { "color": "#d4ffdeff" },

                    "link_text": {
                        "color": "#bcd7ffff",
                        "font_style": "italic"
                    },
                    "link_uri": { "color": "#9ff6ffff" },

                    "emphasis": {
                        "color": "#ff9aa0ff",
                        "font_style": "italic"
                    },
                    "emphasis.strong": {
                        "color": "#ff9aa0ff",
                        "font_weight": 700
                    },

                    "title": { "color": "#e8ecf2ff", "font_weight": 700 },

                    "variable.readonly": { "color": "#e0e3e7ff" },
                    "variable.parameter": { "color": "#9ff6ffff" },

                    "decorator": {
                        "color": "#bcd7ffff",
                        "font_style": "italic"
                    },
                    "namespace": { "color": "#e0e3e7ff" }
                }
            }
        }
    ]
}
