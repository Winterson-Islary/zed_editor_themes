{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Dim Night Elevate – Everforest Fusion",
    "author": "Winterson",
    "themes": [
        {
            "name": "Dim Night Elevate – Everforest Fusion",
            "appearance": "dark",
            "style": {
                "border": "#0e0f0eff",
                "border.variant": "#151715ff",
                "border.focused": "#a7c080ff",
                "border.selected": "#a7c080ff",
                "border.transparent": "#00000000",
                "border.disabled": "#1a1c1aff",

                "elevated_surface.background": "#0c0c0cff",
                "surface.background": "#0c0c0cff",
                "background": "#101010ff",

                "element.background": "#0c0c0cff",
                "element.hover": "#131313ff",
                "element.active": "#1b1b1bff",
                "element.selected": "#1b1b1bff",
                "element.disabled": "#0c0c0cff",

                "text": "#d3c6aaff",
                "text.muted": "#a0a08aff",
                "text.placeholder": "#8b8b77ff",
                "text.disabled": "#707060ff",
                "text.accent": "#a7c080ff",

                "icon": "#d3c6aaff",
                "icon.muted": "#a0a08aff",
                "icon.disabled": "#707060ff",
                "icon.placeholder": "#707060ff",
                "icon.accent": "#a7c080ff",

                "status_bar.background": "#0c0c0cff",
                "title_bar.background": "#0c0c0cff",
                "title_bar.inactive_background": "#0c0c0cff",
                "toolbar.background": "#101010ff",

                "tab_bar.background": "#0c0c0cff",
                "tab.inactive_background": "#0c0c0cff",
                "tab.active_background": "#151515ff",

                "search.match_background": "#a7c08033",
                "panel.background": "#0c0c0cff",
                "panel.focused_border": "#a7c080ff",

                "scrollbar.thumb.background": "#77776633",
                "scrollbar.thumb.hover_background": "#77776644",
                "scrollbar.thumb.border": "#0e0f0eff",
                "scrollbar.track.background": "#00000000",
                "scrollbar.track.border": "#0e0f0eff",

                "editor.foreground": "#d3c6aaff",
                "editor.background": "#101010ff",
                "editor.gutter.background": "#101010ff",
                "editor.subheader.background": "#0c0c0cff",
                "editor.active_line.background": "#1a1a1aff",
                "editor.highlighted_line.background": "#1a1a1aff",
                "editor.line_number": "#5a5a4eff",
                "editor.active_line_number": "#d3c6aaff",
                "editor.hover_line_number": "#c8c0a4ff",
                "editor.invisible": "#5a5a4eff",
                "editor.wrap_guide": "#3a3a3233",
                "editor.active_wrap_guide": "#5a5a4e44",
                "editor.document_highlight.read_background": "#a7c08033",
                "editor.document_highlight.write_background": "#e6987555",

                "terminal.background": "#101010ff",
                "terminal.foreground": "#d3c6aaff",
                "terminal.bright_foreground": "#e1dfc4ff",
                "terminal.dim_foreground": "#8b8b77ff",

                "terminal.ansi.black": "#0c0c0cff",
                "terminal.ansi.red": "#e67e80ff",
                "terminal.ansi.green": "#a7c080ff",
                "terminal.ansi.yellow": "#dbbc7fff",
                "terminal.ansi.blue": "#7fbbb3ff",
                "terminal.ansi.magenta": "#d699b6ff",
                "terminal.ansi.cyan": "#83c092ff",
                "terminal.ansi.white": "#d3c6aaff",

                "terminal.ansi.bright_black": "#5a5a4eff",
                "terminal.ansi.bright_red": "#ec8a8cff",
                "terminal.ansi.bright_green": "#b6d693ff",
                "terminal.ansi.bright_yellow": "#e3c68fff",
                "terminal.ansi.bright_blue": "#9ed3ccff",
                "terminal.ansi.bright_magenta": "#e0aacaff",
                "terminal.ansi.bright_cyan": "#a0d6a7ff",
                "terminal.ansi.bright_white": "#ffffffff",

                "link_text.hover": "#a7c080ff",

                "players": [
                    {
                        "cursor": "#ffffffff",
                        "background": "#ffffffff",
                        "selection": "#ffffff22"
                    }
                ],

                "syntax": {
                    "comment": { "color": "#7a8478ff", "font_style": "italic" },

                    "keyword": { "color": "#7fbbb3ff" },
                    "storage": { "color": "#7fbbb3ff" },

                    "type": { "color": "#a7c080ff" },

                    "number": { "color": "#dbbc7fff" },
                    "constant": { "color": "#dbbc7fff" },

                    "string": { "color": "#a7c080ff" },

                    "variable": { "color": "#ffffffff" },

                    "property": { "color": "#83c092ff" },

                    "function": { "color": "#e69875ff" },

                    "operator": { "color": "#e1dfc4ff" },

                    "tag": { "color": "#e67e80ff" },

                    "attribute": { "color": "#dbbc7fff" },

                    "boolean": { "color": "#dbbc7fff" },

                    "punctuation": { "color": "#e1dfc4ff" },
                    "punctuation.bracket": { "color": "#e1dfc4ff" },
                    "punctuation.delimiter": { "color": "#e1dfc4ff" },

                    "string.regex": { "color": "#83c092ff" },
                    "string.escape": { "color": "#83c092ff" },

                    "text.literal": { "color": "#a7c080ff" },

                    "link_text": {
                        "color": "#9ed3ccff",
                        "font_style": "italic"
                    },
                    "link_uri": { "color": "#83c092ff" },

                    "emphasis": {
                        "color": "#e69875ff",
                        "font_style": "italic"
                    },
                    "emphasis.strong": {
                        "color": "#e69875ff",
                        "font_weight": 700
                    },

                    "title": { "color": "#d3c6aaff", "font_weight": 700 },

                    "variable.readonly": { "color": "#cfc3a1ff" },
                    "variable.parameter": { "color": "#83c092ff" },

                    "decorator": {
                        "color": "#9ed3ccff",
                        "font_style": "italic"
                    },
                    "namespace": { "color": "#cfc3a1ff" }
                }
            }
        }
    ]
}
