                "terminal.ansi.bright_cyan": "#b4fcffff",
                "terminal.ansi.bright_white": "#ffffffff",

                "link_text.hover": "#b06affff",

                "conflict": "#ffe58fff",
                "conflict.background": "#1c1c27ff",
                "conflict.border": "#b06affff",

                "created": "#54ffb7ff",
                "created.background": "#101018ff",
                "created.border": "#54ffb7ff",

                "deleted": "#ff7aaaff",
                "deleted.background": "#101018ff",
                "deleted.border": "#ff7aaaff",

                "error": "#ff7aaaff",
                "error.background": "#101018ff",
                "error.border": "#ff7aaaff",

                "warning": "#ffe58fff",
                "warning.background": "#101018ff",
                "warning.border": "#ffe58fff",

                "info": "#82f6ffff",
                "info.background": "#101018ff",
                "info.border": "#82f6ffff",

                "hint": "#82f6ffff",
                "hint.background": "#101018ff",
                "hint.border": "#82f6ffff",

                "players": [
                    {
                        "cursor": "#ffffffff",
                        "background": "#ffffffff",
                        "selection": "#ffffff33"
                    }
                ],

                "syntax": {
                    "comment": { "color": "#9e94b3ff", "font_style": "italic" },
                    "keyword": { "color": "#c8a1ffff" },
                    "storage": { "color": "#c8a1ffff" },
                    "type": { "color": "#bc9dffff" },
                    "number": { "color": "#ffdf9aff" },
                    "constant": { "color": "#ffdf9aff" },
                    "string": { "color": "#f1b0ffff" },
                    "variable": { "color": "#ffffffff" },
                    "property": { "color": "#d499ffff" },
                    "function": { "color": "#ff9df7ff" },
                    "operator": { "color": "#e3dfebff" },
                    "tag": { "color": "#ff7aaaff" },
                    "attribute": { "color": "#ffdf9aff" },
                    "boolean": { "color": "#ffdf9aff" },
                    "punctuation": { "color": "#e3dfebff" },
                    "punctuation.bracket": { "color": "#e3dfebff" },
                    "punctuation.delimiter": { "color": "#e3dfebff" },
                    "string.regex": { "color": "#d9bdffff" },
                    "string.escape": { "color": "#d9bdffff" },
                    "text.literal": { "color": "#f1b0ffff" },
                    "link_text": {
                        "color": "#ccb5ffff",
                        "font_style": "italic"
                    },
                    "link_uri": { "color": "#b06affff" },
                    "emphasis": {
                        "color": "#ff7aaaff",
                        "font_style": "italic"
                    },
                    "emphasis.strong": {
                        "color": "#ff7aaaff",
                        "font_weight": 700
                    },
                    "title": { "color": "#e6e1f7ff", "font_weight": 700 },
                    "variable.readonly": { "color": "#e2dff0ff" },
                    "variable.parameter": { "color": "#b06affff" },
                    "decorator": {
                        "color": "#ccb5ffff",
                        "font_style": "italic"
                    },
                    "namespace": { "color": "#e2dff0ff" }
                }
