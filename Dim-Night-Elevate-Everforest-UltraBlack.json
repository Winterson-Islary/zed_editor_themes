{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Dim Night Elevate – Everforest UltraBlack",
    "author": "Winterson",
    "themes": [
        {
            "name": "Dim Night Elevate – Everforest UltraBlack",
            "appearance": "dark",
            "style": {
                "border": "#000000ff",
                "border.variant": "#0a0a0aff",
                "border.focused": "#a7c080ff",
                "border.selected": "#a7c080ff",
                "border.transparent": "#00000000",
                "border.disabled": "#0f0f0fff",

                "elevated_surface.background": "#000000ff",
                "surface.background": "#000000ff",
                "background": "#000000ff",

                "element.background": "#000000ff",
                "element.hover": "#111111ff",
                "element.active": "#1a1a1aff",
                "element.selected": "#1a1a1aff",
                "element.disabled": "#000000ff",

                "text": "#d3c6aaff",
                "text.muted": "#a0a08aff",
                "text.placeholder": "#8b8b77ff",
                "text.disabled": "#707060ff",
                "text.accent": "#a7c080ff",

                "icon": "#d3c6aaff",
                "icon.muted": "#a0a08aff",
                "icon.disabled": "#707060ff",
                "icon.placeholder": "#707060ff",
                "icon.accent": "#a7c080ff",

                "status_bar.background": "#000000ff",
                "title_bar.background": "#000000ff",
                "title_bar.inactive_background": "#000000ff",
                "toolbar.background": "#000000ff",

                "tab_bar.background": "#000000ff",
                "tab.inactive_background": "#000000ff",
                "tab.active_background": "#111111ff",

                "search.match_background": "#a7c08044",
                "panel.background": "#000000ff",
                "panel.focused_border": "#a7c080ff",

                "scrollbar.thumb.background": "#77776633",
                "scrollbar.thumb.hover_background": "#77776644",
                "scrollbar.thumb.border": "#000000ff",
                "scrollbar.track.background": "#00000000",
                "scrollbar.track.border": "#000000ff",

                "editor.foreground": "#d3c6aaff",
                "editor.background": "#000000ff",
                "editor.gutter.background": "#000000ff",
                "editor.subheader.background": "#000000ff",
                "editor.active_line.background": "#141414ff",
                "editor.highlighted_line.background": "#141414ff",
                "editor.line_number": "#59594eff",
                "editor.active_line_number": "#d3c6aaff",
                "editor.hover_line_number": "#cfc4abff",
                "editor.invisible": "#59594eff",
                "editor.wrap_guide": "#33333333",
                "editor.active_wrap_guide": "#59594e44",
                "editor.document_highlight.read_background": "#a7c08033",
                "editor.document_highlight.write_background": "#e6987544",

                "terminal.background": "#000000ff",
                "terminal.foreground": "#d3c6aaff",
                "terminal.bright_foreground": "#e1dfc4ff",
                "terminal.dim_foreground": "#8b8b77ff",

                "terminal.ansi.black": "#000000ff",
                "terminal.ansi.red": "#e67e80ff",
                "terminal.ansi.green": "#a7c080ff",
                "terminal.ansi.yellow": "#dbbc7fff",
                "terminal.ansi.blue": "#7fbbb3ff",
                "terminal.ansi.magenta": "#d699b6ff",
                "terminal.ansi.cyan": "#83c092ff",
                "terminal.ansi.white": "#d3c6aaff",

                "terminal.ansi.bright_black": "#4b4b40ff",
                "terminal.ansi.bright_red": "#ec8a8cff",
                "terminal.ansi.bright_green": "#b6d693ff",
                "terminal.ansi.bright_yellow": "#e3c68fff",
                "terminal.ansi.bright_blue": "#9ed3ccff",
                "terminal.ansi.bright_magenta": "#e0aacaff",
                "terminal.ansi.bright_cyan": "#a0d6a7ff",
                "terminal.ansi.bright_white": "#ffffffff",

                "conflict": "#e67e80ff",
                "conflict.background": "#00000000",
                "conflict.border": "#ff6a6aff",

                "created": "#a7c080ff",
                "created.background": "#00000000",
                "created.border": "#46e06aff",

                "deleted": "#e67e80ff",
                "deleted.background": "#00000000",
                "deleted.border": "#ff6a6aff",

                "error": "#e67e80ff",
                "error.background": "#00000000",
                "error.border": "#ff6a6aff",

                "warning": "#dbbc7fff",
                "warning.background": "#00000000",
                "warning.border": "#e3c068ff",

                "info": "#83c092ff",
                "info.background": "#00000000",
                "info.border": "#3ac6e6ff",

                "hint": "#83c092ff",
                "hint.background": "#00000000",
                "hint.border": "#3ae69aff",

                "link_text.hover": "#a7c080ff",

                "players": [
                    {
                        "cursor": "#ffffffff",
                        "background": "#ffffffff",
                        "selection": "#ffffff22"
                    }
                ],

                "syntax": {
                    "comment": { "color": "#7a8478ff", "font_style": "italic" },

                    "keyword": { "color": "#7fbbb3ff" },
                    "storage": { "color": "#7fbbb3ff" },

                    "type": { "color": "#a7c080ff" },

                    "number": { "color": "#dbbc7fff" },
                    "constant": { "color": "#dbbc7fff" },

                    "string": { "color": "#a7c080ff" },

                    "variable": { "color": "#ffffffff" },

                    "property": { "color": "#83c092ff" },

                    "function": { "color": "#e69875ff" },

                    "operator": { "color": "#e1dfc4ff" },

                    "tag": { "color": "#e67e80ff" },

                    "attribute": { "color": "#dbbc7fff" },
                    "boolean": { "color": "#dbbc7fff" },

                    "punctuation": { "color": "#e1dfc4ff" },
                    "punctuation.bracket": { "color": "#e1dfc4ff" },
                    "punctuation.delimiter": { "color": "#e1dfc4ff" },

                    "string.regex": { "color": "#83c092ff" },
                    "string.escape": { "color": "#83c092ff" },

                    "text.literal": { "color": "#a7c080ff" },

                    "link_text": {
                        "color": "#9ed3ccff",
                        "font_style": "italic"
                    },
                    "link_uri": { "color": "#83c092ff" },

                    "emphasis": {
                        "color": "#e69875ff",
                        "font_style": "italic"
                    },
                    "emphasis.strong": {
                        "color": "#e69875ff",
                        "font_weight": 700
                    },

                    "title": { "color": "#d3c6aaff", "font_weight": 700 },

                    "variable.readonly": { "color": "#cfc3a1ff" },
                    "variable.parameter": { "color": "#83c092ff" },

                    "decorator": {
                        "color": "#9ed3ccff",
                        "font_style": "italic"
                    },
                    "namespace": { "color": "#cfc3a1ff" }
                }
            }
        }
    ]
}
