{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "name": "Dim Night Elevate – Warm Black",
    "author": "Winterson",
    "themes": [
        {
            "name": "Dim Night Elevate – Warm Black",
            "appearance": "dark",
            "style": {
                "border": "#15110eff",
                "border.variant": "#1d1915ff",
                "border.focused": "#ffb46bff",
                "border.selected": "#ffb46bff",
                "border.transparent": "#00000000",
                "border.disabled": "#1a1714ff",

                "elevated_surface.background": "#0d0a08ff",
                "surface.background": "#0d0a08ff",
                "background": "#110e0cff",

                "element.background": "#0d0a08ff",
                "element.hover": "#16120fff",
                "element.active": "#1e1814ff",
                "element.selected": "#1e1814ff",
                "element.disabled": "#0d0a08ff",

                "text": "#f2ece6ff",
                "text.muted": "#b0a79fff",
                "text.placeholder": "#91877eff",
                "text.disabled": "#7a726bff",
                "text.accent": "#ffb46bff",

                "icon": "#f2ece6ff",
                "icon.muted": "#b0a79fff",
                "icon.disabled": "#7a726bff",
                "icon.placeholder": "#7a726bff",
                "icon.accent": "#ffb46bff",

                "status_bar.background": "#0d0a08ff",
