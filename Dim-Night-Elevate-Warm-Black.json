                    "comment": { "color": "#b0a79fff", "font_style": "italic" },
                    "keyword": { "color": "#ffc28dff" },
                    "storage": { "color": "#ffc28dff" },
                    "type": { "color": "#ffe8baff" },
                    "number": { "color": "#ffd8aaff" },
                    "constant": { "color": "#ffd8aaff" },
                    "string": { "color": "#ffe3c5ff" },
                    "variable": { "color": "#ffffffff" },
                    "property": { "color": "#ffd1aaff" },
                    "function": { "color": "#ffb46bff" },
                    "operator": { "color": "#e7e1dbff" },
                    "tag": { "color": "#ff988cff" },
                    "attribute": { "color": "#ffd8aaff" },
                    "boolean": { "color": "#ffd8aaff" },
                    "punctuation": { "color": "#e7e1dbff" },
