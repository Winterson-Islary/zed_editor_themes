                "deleted.border": "#ff8577ff",

                "error": "#ff8577ff",
                "error.background": "#110e0cff",
                "error.border": "#ff8577ff",

                "warning": "#ffe49dff",
                "warning.background": "#110e0cff",
                "warning.border": "#ffe49dff",

                "info": "#bafaffff",
