                "scrollbar.thumb.border": "#15110eff",
                "scrollbar.track.background": "#00000000",
                "scrollbar.track.border": "#15110eff",

                "editor.foreground": "#f2ece6ff",
                "editor.background": "#110e0cff",
                "editor.gutter.background": "#110e0cff",
                "editor.subheader.background": "#0d0a08ff",
                "editor.active_line.background": "#1b1612ff",
