                    "punctuation.delimiter": { "color": "#e7e1dbff" },
                    "string.regex": { "color": "#ffe9ccff" },
                    "string.escape": { "color": "#ffe9ccff" },
                    "text.literal": { "color": "#ffe3c5ff" },
                    "link_text": {
                        "color": "#ffd7b3ff",
                        "font_style": "italic"
                    },
                    "link_uri": { "color": "#ffb46bff" },
                    "emphasis": {
                        "color": "#ff988cff",
                        "font_style": "italic"
                    },
                    "emphasis.strong": {
                        "color": "#ff988cff",
                        "font_weight": 700
                    },
                    "title": { "color": "#f2ece6ff", "font_weight": 700 },
                    "variable.readonly": { "color": "#eae6e2ff" },
                    "variable.parameter": { "color": "#ffb46bff" },
                    "decorator": {
                        "color": "#ffd7b3ff",
                        "font_style": "italic"
                    },
                    "namespace": { "color": "#eae6e2ff" }
