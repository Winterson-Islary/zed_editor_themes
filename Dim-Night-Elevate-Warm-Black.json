                "toolbar.background": "#110e0cff",

                "tab_bar.background": "#0d0a08ff",
                "tab.inactive_background": "#0d0a08ff",
                "tab.active_background": "#181410ff",

                "search.match_background": "#ffb46b33",
                "panel.background": "#0d0a08ff",
                "panel.focused_border": "#ffb46bff",

                "scrollbar.thumb.background": "#b0a79f33",
