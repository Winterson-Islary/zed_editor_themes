                "info.border": "#bafaffff",

                "hint": "#bafaffff",
                "hint.background": "#110e0cff",
                "hint.border": "#bafaffff",

                "players": [
                    {
                        "cursor": "#ffffffff",
                        "background": "#ffffffff",
                        "selection": "#ffffff33"
                    }
                ],

